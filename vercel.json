{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "/renew_limit",
      "schedule": "45 16 * * *"
    },
    {
      "path": "/pay_loan",
      "schedule": "2 0 1 * *"
    }
  ]
}
